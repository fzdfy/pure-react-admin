{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.5.0",
    "generators": {
      "v3.0": {
        "generatorName": "typescript-fetch",
        "output": "./src/request",
        "glob": "./swagger/swagger.json",
        "additionalProperties": {
          "supportsES6": true,
          "withInterfaces": true
        }
      }
    }
  }
}
